{
  "code-runner.executorMap": {
    "typescript": "\"$workspaceRoot/node_modules/.bin/ts-node\" $fullFileName"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "flow.pathToFlow": "${workspaceFolder}/node_modules/.bin/flow",
  "javascript.validate.enable": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#05444d",
    "tab.activeBorder": "#05444d",
    "titleBar.activeBackground": "#05444d",
    "titleBar.border": "#05444d",
    "titleBar.inactiveBackground": "#05444d"
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
